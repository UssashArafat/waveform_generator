#include "xil_io.h"

int main()
{
    init_platform();

    //16 bit input parameters
    short finalValue = 200;
	short dropValue = 150;
	short hValue = 10;
	short vValue = 5;
	short period = 100;
	printf("finalValue = %d", finalValue);
    cleanup_platform();
    return 0;
}
